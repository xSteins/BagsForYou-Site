                                <label for="categoryIn">Category</label>
                                <input type="text" id="sub-categoryIn" name="sub-categoryIn">
                                <label for="sub-categoryIn">Sub-category</label>
                                <input type="submit" id="category-submit">
                        </form>
                    </div>
                </div>
                <div class="secondary-menu">
                    <%- include('../adminMenus/stat-all') %>
                </div>
            </div>
            <div class="right-column">
                <%- include('../adminMenus/recentBags') %>
            </div>
        </div>
